client
dev tun
proto udp
nobind
remote-random
remote us-atl.securetunnels.net 443
remote us-atl.torguard.org 443
ca ca.crt
auth-user-pass
remote-cert-tls server
cipher AES-256-CBC
resolv-retry 5
tun-mtu 24000
fragment 0
mssfix 0
route-delay 5 30
ping-restart 0
keepalive 5 30
comp-lzo
mute-replay-warnings
fast-io
sndbuf 524288
rcvbuf 524288
